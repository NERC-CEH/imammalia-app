{
  "name": "imammalia-app",
  "id": "uk.ac.ceh.imammalia",
  "title": "iMammalia App",
  "description": "iMammalia mobile application.",
  "version": "1.4.0",
  "homepage": "https://european-mammals.brc.ac.uk",
  "scripts": {
    "start": "webpack serve --progress --port 8000",
    "build": "NODE_ENV=production webpack",
    "build:production": "npm run clean && npm run build && npx cap sync && npm run build:resources",
    "build:resources": "mkdir -p resources && cp -R other/designs/android resources && cp -R other/designs/*.png resources && npx cordova-res ios --skip-config --resources resources --copy && npx cordova-res android --skip-config --resources resources --copy",
    "deploy": "vercel --prod -A other/now.json -t $VERCEL_TOKEN --confirm",
    "deploy:production": "echo 'NOTHING HERE :('",
    "test": "karma start test/karma.conf.js",
    "clean": "rm -rf build && rm -rf resources && rm -rf cordova",
    "test:watch": "karma start test/karma.conf.js --no-single-run",
    "test:sauce": "karma start test/karma.conf-sauce.js",
    "test:eslint": "eslint src --quiet --ext .jsx --ext .js && echo '\\033[37;42;1m PASS \\033[00m'",
    "fetch:data": "cd src/common/data && node make.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:eslint && npm run test"
    }
  },
  "dependencies": {
    "@capacitor/android": "^3.4.3",
    "@capacitor/app": "^1.1.1",
    "@capacitor/core": "^3.7.0",
    "@capacitor/filesystem": "^1.1.0",
    "@capacitor/ios": "^3.4.3",
    "@capacitor/haptics": "^1.1.4",
    "@capacitor/camera": "^1.3.1",
    "@capacitor/network": "^1.0.7",
    "@capacitor/splash-screen": "^1.2.2",
    "@capacitor/status-bar": "^1.0.8",
    "@flumens/ionic": "1.6.1",
    "@indicia-js/core": "^6.1.0",
    "@indicia-js/remote": "^6.1.5",
    "@ionic-native/core": "5.36.0",
    "@ionic-native/sqlite": "5.36.0",
    "@ionic/core": "^6.1.4",
    "@ionic/react": "^6.1.4",
    "@ionic/react-router": "^6.1.4",
    "@sentry/browser": "^6.19.7",
    "@sentry/integrations": "^6.19.7",
    "clsx": "^1.1.1",
    "core-js": "3.2.1",
    "formik": "2.2.9",
    "i18next": "^21.8.9",
    "indicia": "4.4.2",
    "ionicons": "^6",
    "leaflet": "1.7.1",
    "localforage": "1.10.0",
    "localforage-cordovasqlitedriver": "1.8.0",
    "lodash": "4.17.15",
    "mobx": "^6.6.0",
    "mobx-react": "^7.5.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.17.1",
    "react-leaflet": "3.2.2",
    "react-photoswipe": "^1.3.0",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-exponentiation-operator": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.16.7",
    "@flumens/eslint-config": "^2.0.1",
    "@flumens/fetch-onedrive-excel": "^0.1.0",
    "@flumens/webpack-config": "^4.1.2",
    "@types/jest": "^27.5.1",
    "@types/leaflet": "^1.7.10",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-table": "^7.7.12",
    "@types/react-window": "^1.8.5",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "chai": "^4.3.6",
    "cordova-res": "^0.15.3",
    "csvtojson": "2.0.10",
    "dotenv": "^16.0.1",
    "eslint": "7.21.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "express": "^4.18.1",
    "express-history-api-fallback": "2.2.1",
    "husky": "3.0.7",
    "jest": "^26.6.3",
    "mock-geolocation": "1.0.11",
    "po2json": "^1.0.0-beta-3",
    "prettier": "^2.0.2",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.1",
    "puppeteer": "^8.0.0",
    "sinon": "7.5.0",
    "typescript": "^4.6.4",
    "vercel": "21.2.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "xml2js": "0.4.22"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "author": {
    "name": "Flumens",
    "email": "enquiries@flumens.io",
    "url": "https://flumens.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NERC-CEH/imammalia-app"
  },
  "bugs": {
    "url": "https://github.com/NERC-CEH/imammalia-app/issues"
  },
  "licenses": [
    {
      "type": "GNU GPL v3",
      "url": "http://www.gnu.org/licenses/gpl.html"
    }
  ],
  "keywords": [
    "imammalia",
    "biological",
    "recording",
    "mobile",
    "app",
    "offline",
    "ceh"
  ],
  "babel": {
    "presets": [
      [
        "@babel/react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/typescript",
      [
        "@babel/env",
        {
          "targets": {
            "android": "5.1",
            "ios": "10.2"
          },
          "modules": "commonjs",
          "corejs": "3.0.0",
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@flumens$": "./src/common/flumens",
            "common": "./src/common",
            "helpers": "./src/common/helpers",
            "models": "./src/common/models",
            "app_model": "./src/common/models/app_model",
            "user_model": "./src/common/models/user_model",
            "sample": "./src/common/models/sample",
            "occurrence": "./src/common/models/occurrence",
            "saved_samples": "./src/common/saved_samples",
            "config": "./src/common/config/config",
            "Components": "./src/common/Components"
          }
        }
      ],
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-transform-exponentiation-operator",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@flumens",
      "plugin:react/jsx-runtime"
    ],
    "rules": {
      "react/function-component-definition": 0,
      "react/prop-types": 0,
      "react/jsx-no-bind": 0,
      "@typescript-eslint/no-var-requires": 0
    }
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "babel-jest",
        {
          "presets": [
            "@babel/env"
          ]
        }
      ]
    },
    "testRegex": "((src/\\.|/*.)(spec))\\.(t|j)s?$",
    "moduleNameMapper": {
      "^.+.(css|style|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "ionic-native": "jest-transform-stub",
      "swiper": "jest-transform-stub"
    },
    "moduleDirectories": [
      "node_modules",
      "components",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic|@stencil|ionicons)"
    ]
  }
}
